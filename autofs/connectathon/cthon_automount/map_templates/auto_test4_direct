#
# Direct map for automounter connectathon testing
#

#
# POSITIVE TESTS
#

#
# basic tests
#
AUTO_CLIENT_MNTPNT/test4/direct/g1				SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g1a	/			SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g1b				SERVER1:AUTO_SERVER_DIR/export1/

#
# option support
#
AUTO_CLIENT_MNTPNT/test4/direct/g2		-ro		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g2b		-rw		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g2c	/	-ro		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g2d	-ro \
	/				SERVER1:AUTO_SERVER_DIR/export1

#
# subdir support
#
AUTO_CLIENT_MNTPNT/test4/direct/g3	 \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/g3a	 \
	/				SERVER1:AUTO_SERVER_DIR/export5/testing/test \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/g3b				SERVER1:AUTO_SERVER_DIR/export5/testing/test \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/g3c	 \
	/				SERVER1:AUTO_SERVER_DIR/export5/testing/test \
	/s1				SERVER1:AUTO_SERVER_DIR/export/testing/test/s1 \
	/s2				SERVER1:AUTO_SERVER_DIR/export5/testing/test/s2 \
	/s1/ss1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2/ss2				SERVER1:AUTO_SERVER_DIR/export2
AUTO_CLIENT_MNTPNT/test4/direct/g3d	 \
	/				SERVER1:AUTO_SERVER_DIR/export5/testing/test/s1 \
	/s1				SERVER1:AUTO_SERVER_DIR/export2

#
# subdir-option support 
#
AUTO_CLIENT_MNTPNT/test4/direct/g4	-ro  \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/g4a	-ro  \
	/				SERVER1:AUTO_SERVER_DIR/export5/testing/test \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/g4b	-ro 			SERVER1:AUTO_SERVER_DIR/export5/testing/test \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3

#
# offset support
#
AUTO_CLIENT_MNTPNT/test4/direct/g5	 \
	/s1/ss1				SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g5a	/s1/ss1			SERVER1:AUTO_SERVER_DIR/export5/testing/test/s1/ss1 \
	/s1/ss1/sss1/			SERVER1:AUTO_SERVER_DIR/export5/testing/test/s1/ss1/sss1 \
	/s1/ss1/sss1/ssss1/sssss1	SERVER1:AUTO_SERVER_DIR/export2

#
# replicated server support
#
AUTO_CLIENT_MNTPNT/test4/direct/g6				SERVER1,SERVER2:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g6a				SERVER1:AUTO_SERVER_DIR/export1 \
					SERVER2:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g6b				SERVER1:AUTO_SERVER_DIR/export1 \
					SERVER2:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g6c 				SERVER1,SERVER2(10):AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g6d	 \
	/s1				SERVER1,SERVER2:AUTO_SERVER_DIR/export1

#
# multihomed host support
#

#
# macro support (also provided a wildcard at the end)
#
AUTO_CLIENT_MNTPNT/test4/direct/g8	/$ARCH			SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g8a	/$CPU			SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g8b	/$HOST			SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g8c	/$OSREL			SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g8d	/$OSNAME		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g8e	/$OSVERS		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g8f	/$CPU$OSVERS		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g8g	/$HOST			$HOST:AUTO_SERVER_DIR/

#
# special characters
#
AUTO_CLIENT_MNTPNT/test4/direct/g9				SERVER1:AUTO_SERVER_DIR/"export1"
AUTO_CLIENT_MNTPNT/test4/direct/g9a				SERVER1:AUTO_SERVER_DIR/export5/testing/test/exp#ort1
AUTO_CLIENT_MNTPNT/test4/direct/g9b				SERVER1:AUTO_SERVER_DIR/export1	# comment test
AUTO_CLIENT_MNTPNT/test4/direct/g9c				SERVER1:AUTO_SERVER_DIR//export1
AUTO_CLIENT_MNTPNT/test4/direct/g9-d				SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g9#e				SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/"/test4/direct/g9f"				SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/g9g				SERVER1:AUTO_SERVER_DIR/export1////
AUTO_CLIENT_MNTPNT/test4/direct/g9j   \
        //s1			       	SERVER1:AUTO_SERVER_DIR/export5/testing/test \
        //s2				SERVER1:AUTO_SERVER_DIR/export5/testing/test

#
# long paths
#
AUTO_CLIENT_MNTPNT/test4/direct/g10b				SERVER1:AUTO_SERVER_DIR/export5/testing/test/s1/ss1/sss1/ssss1/sssss1

#
# NEGATIVE PARSER TESTS
#

