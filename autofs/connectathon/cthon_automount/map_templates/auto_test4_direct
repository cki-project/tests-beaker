AUTO_CLIENT_MNTPNT/test4/direct/b1b				SERVER1/export1
AUTO_CLIENT_MNTPNT/test4/direct/b1c				/SERVER1
AUTO_CLIENT_MNTPNT/test4/direct/b1d	/			/SERVER1

#
# option support
#
AUTO_CLIENT_MNTPNT/test4/direct/b2	/	-ro
AUTO_CLIENT_MNTPNT/test4/direct/b2a		-ro		/SERVER1
AUTO_CLIENT_MNTPNT/test4/direct/b2b	/	-ro		/SERVER1
AUTO_CLIENT_MNTPNT/test4/direct/b2c		-ro		SERVER1/export1
AUTO_CLIENT_MNTPNT/test4/direct/b2d		ro		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/b2e	/	ro		SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/b2f		-ro


#
# subdir support
#
AUTO_CLIENT_MNTPNT/test4/direct/b3	    \
	/s1				SERVER1:AUTO_SERVER_DIR/export1 \
	/s1				SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/b3b	 \
	/b3c
AUTO_CLIENT_MNTPNT/test4/direct/b3c	 \
	/				
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/b3d	/s1	-ro			SERVER1:AUTO_SERVER_DIR/export1 \
	/s2				SERVER1:AUTO_SERVER_DIR/export2 \	
	/s1				SERVER1:AUTO_SERVER_DIR/export3	

#
# subdir-option support
#
AUTO_CLIENT_MNTPNT/test4/direct/b4	    \
	/s1	-ro			SERVER1:AUTO_SERVER_DIR/export1 \
	/s1	-ro			SERVER1:AUTO_SERVER_DIR/export1
AUTO_CLIENT_MNTPNT/test4/direct/b4a	/	-ro  \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/b4b	-ro 				/SERVER1:AUTO_SERVER_DIR/export1 \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \
	/s2				SERVER1:AUTO_SERVER_DIR/export3
AUTO_CLIENT_MNTPNT/test4/direct/b4c	-ro 				/SERVER1:AUTO_SERVER_DIR/export1 \
	/s1				SERVER1:AUTO_SERVER_DIR/export2 \

#
# offset support
#
AUTO_CLIENT_MNTPNT/test4/direct/b5	 \			SERVER1:AUTO_SERVER_DIR/export6 \
	/s1/ssbad			SERVER1:AUTO_SERVER_DIR/export6
AUTO_CLIENT_MNTPNT/test4/direct/b5a	/s1/ss1			SERVER1:AUTO_SERVER_DIR/export6 \
	/s1/ss1/sss1/ssss1/sssssbad	SERVER1:AUTO_SERVER_DIR/export2

#
# replicated server support
#


#
# macro support
#
AUTO_CLIENT_MNTPNT/test4/direct/b8	/$ARCB			SERVER1:AUTO_SERVER_DIR/export1

#
# special character support
#
AUTO_CLIENT_MNTPNT/test4/direct/b9c \
	*				SERVER1:AUTO_SERVER_DIR/export1/
AUTO_CLIENT_MNTPNT/test4/direct/b9e   \
        /  s1			       	SERVER1:AUTO_SERVER_DIR/export \
        /  s2				SERVER1:AUTO_SERVER_DIR/export1
